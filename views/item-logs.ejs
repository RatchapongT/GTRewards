<!DOCTYPE html>
<html lang="en">
    <% include partials/page/jsdefaults.ejs %>

    <head>
        <% include partials/page/head.ejs %>
    </head>
    <body class="home" style="background-image: url(assets/images/buzz.png); background-position: center center;
             background-repeat: no-repeat; background-attachment: fixed; background-size: cover;">
        <% include partials/page/navbar.ejs %>

        <div class="container" style="padding-top: 200px">
            <table class="table table-striped table-hover"
                   style="width: 100%; color:black; text-shadow: none; " >
                <thead>
                    <tr class="warning">
                        <th style="text-align: center">Date</th>
                        <th style="text-align: center">gtID</th>
                        <th style="text-align: center">Last Name</th>
                        <th style="text-align: center">First Name</th>
                        <th style="text-align: center">Email</th>
                        <th style="text-align: center">Student Points</th>
                        <th style="text-align: center">Price</th>
                        <th style="text-align: center">Quantity</th>
                        <th style="text-align: center">Log</th>
                    </tr>
                </thead>
                <tbody>

                    <% for (var i = 0 ; i < itemLogObject.length; i++) { %>
                    <tr class="success">
                        <td><%= moment(new Date(itemLogObject[i].created)).format(shortDateFormat) %></td>
                        <td><%= itemLogObject[i].gtID %></td>
                        <td><%= itemLogObject[i].lastName %></td>
                        <td><%= itemLogObject[i].firstName %></td>
                        <td><%= itemLogObject[i].email %></td>
                        <td><%= itemLogObject[i].sum %></td>
                        <td><%= itemLogObject[i].price %></td>
                        <td><%= itemLogObject[i].quantityRequested %></td>
                        <td><%= itemLogObject[i].description %></td>

                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>



    </body>

</html>
<script>
    $(document).ready(function () {
        $('td.date').each(function (index) {
            var today = new Date();
            var m = today.getMonth();
            var d = today.getDate();
            var y = today.getFullYear();
            var date = new Date(y, m, d);
            if (new Date($(this).text()) < date) {
                $(this).parent().attr("style", "display: none")
            }
        });
    });
</script>
